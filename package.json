{
    "name": "arkhe-dev",
    "version": "1.0.0",
    "description": "my gulp modules",
    "main": "index.js",
    "scripts": {
        "wp-start": "wp-env start && open http://localhost:2222",
        "wp-stop": "wp-env run cli wp db export sql/local.sql && wp-env stop",
        "wp-inport": "wp-env run cli wp db inport",
        "build": "run-s build:js build:adminJs build:css",
        "build:css": "run-s scss postcss",
        "build:js": "npx webpack",
        "build:adminJs": "npx webpack --config webpack.config.admin.js",
        "scss": "run-s scss:main scss:editor scss:module/-overlay-header scss:admin/menu scss:admin/customizer scss:admin/nav-menus scss:admin/edit-table",
        "scss:main": "node-sass src/scss/main.scss dist/css/main.css --importer node_modules/node-sass-globbing/index.js",
        "scss:editor": "node-sass src/scss/editor.scss dist/css/editor.css --importer node_modules/node-sass-globbing/index.js",
        "scss:module/-overlay-header": "node-sass src/scss/module/-overlay-header.scss dist/css/module/-overlay-header.css",
        "scss:admin/menu": "node-sass src/scss/admin/menu.scss dist/css/admin/menu.css",
        "scss:admin/customizer": "node-sass src/scss/admin/customizer.scss dist/css/admin/customizer.css",
        "scss:admin/nav-menus": "node-sass src/scss/admin/nav-menus.scss dist/css/admin/nav-menus.css",
        "scss:admin/edit-table": "node-sass src/scss/admin/edit-table.scss dist/css/admin/edit-table.css",
        "postcss": "postcss dist/css/*.css -d dist/css && postcss dist/css/admin/*.css -d dist/css/admin && postcss dist/css/module/*.css -d dist/css/module",
        "watch": "npx npm-run-all --parallel watch:css watch:js watch:adminJs",
        "watch:css": "nodemon --watch src -e scss -x \"npm run build:css\"",
        "watch:js": "nodemon --watch src/js --ignore **/plugin/** --ignore **/admin/** -e js -x \"npm run build:js\"",
        "watch:adminJs": "nodemon --watch src/js/admin -e js -x \"npm run build:adminJs\"",
        "update": "bash ./bin/zip.sh"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@wordpress/browserslist-config": "^2.7.0",
        "@wordpress/eslint-plugin": "^7.2.0",
        "autoprefixer": "^9.8.6",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.1.0",
        "classnames": "^2.2.6",
        "core-js": "^3.6.5",
        "css-mqpacker": "^7.0.0",
        "cssnano": "^4.1.10",
        "eslint": "^7.8.1",
        "node-sass": "^4.14.1",
        "node-sass-globbing": "0.0.23",
        "nodemon": "^2.0.4",
        "npm-check-updates": "^8.1.1",
        "npm-run-all": "^4.1.5",
        "po2json": "^1.0.0-beta-2",
        "postcss": "^8.2.1",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.1.2",
        "regenerator-runtime": "^0.13.7",
        "stylelint": "^13.7.0",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-wordpress": "^17.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {},
    "browserslist": [
        "extends @wordpress/browserslist-config",
        "ie > 11"
    ]
}
